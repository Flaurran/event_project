        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/app.css') }}">
    </head>
    <body>
    {% block menu %}{% endblock %}
    {% block message %}{% endblock %}
    {% block content %}{% endblock %}
    {% block footer %}{% endblock %}
    {% block javascripts %}{% endblock %}
    </body>
</html>
